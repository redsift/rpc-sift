{
  "name": "rpc-sift",
  "display-name": "RPC Demo Sift",
  "description": "",
  "author": "",
  "version": "1.0.0",
  "icon": "assets/blueprint.svg",
  "interfaces": {
    "summary": {
      "controller": "dist/js/controller.umd-es2015.min.js"
    }
  },
  "dag": {
    "inputs":{
      "rpc":{
        "echo":{
          "methods":["GET", "POST"],
          "path":"/echo"
        }
      }
    },
    "nodes":[{
      "#": "RPC",
      "implementation": {
        "javascript": "server/rpc.js"
      },
      "input": {
        "bucket": "echo"
      },
      "outputs": {
        "api_rpc": {}
      }
    }],
    "stores":{
    },
    "outputs":{
      "exports":{
        "api_rpc":{
          "import":"_rpc"
        }
      }
    }
  }
}
